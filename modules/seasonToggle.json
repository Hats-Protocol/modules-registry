{
  "name": "Season Toggle",
  "description": "Allows an organization to configure certain hats to be automatically toggled off after a given interval, i.e. a 'season'.",
  "github": {
    "owner": "Hats-Protocol",
    "repo": "season-toggle"
  },
  "type": {
    "eligibility": false,
    "toggle": true,
    "hatter": false
